ned(this.$modalStack)){\n      this.$log.debug(\"dismissing modal windows\");\n      this.$modalStack.dismissAll(\"authentication required\");\n    }\n\n    this.clearNextState();\n\n    if (nextState) {\n      this.nextState = nextState.name;\n      this.nextStateParams = nextStateParams;\n    } else {\n      this.nextState = this.containerState.current.name;\n      this.nextStateParams = this.containerState.params;\n    }\n\n    if (error)\n      this.containerState.goSignonState({errorMessage: error.message});\n    else\n      this.containerState.goSignonState();\n  }\n\n  private redirectToConnectingPage() {\n    this.$log.debug(\"Redirecting to connecting-state\");\n    this.containerState.goConnectingState();\n  }\n\n  private redirectToErrorPage() {\n    this.$log.debug(\"Redirecting to error-state\");\n    this.containerState.goErrorState();\n  }\n\n  private resumeNextPage() {\n    if (this.nextState) {\n      this.$log.debug(\"Resuming with \" + this.nextState);\n      this.containerState.go(this.nextState, this.nextStateParams);\n    } else {\n      this.$log.debug(\"Resuming with home-state\");\n      this.containerState.goHomeState();\n    }\n  }\n\n  private reloadCurrentPage() {\n    this.$log.debug('reloadCurrentPage, state=' + this.containerState.current.name);\n    this.containerState.go(this.containerState.current.name, this.containerState.params, { reload: true });\n  }\n}\n\nclass StateInterceptorProvider implements ng.IServiceProvider {\n\n  constructor() { }\n\n  public $get = [\"$rootScope\", \"$log\", \"$location\", \"$modalStack\", \"applicationState\", \"authenticationService\", ($rootScope, $log, $location, $modalStack, containerState, authenticationService) => {\n    var result: any = new StateInterceptor($rootScope, $log, $location, $modalStack, containerState, authenticationService);\n    return result;\n  }];\n}\n\nexport = StateInterceptorProvider;\n","markerStore":{"nextMarkerId":111,"markersById":{"0":{"range":{"start":{"row":13,"c